    "songs": [
        {
            "title": "Kylm\u00e4\u00e4 kyyti\u00e4",
            "year": "2023",
            "lyrics": [
                "Sauvani valossa",
                "usvan l\u00e4pi n\u00e4\u00e4tk\u00f6 kuolleet jotka",
                "t\u00e4\u00e4ll\u00e4 ikuisesti vaeltaa?",
                "Ku\u2019uletko kuiskeensa?",
                "Ja varoitukset jotka ilman l\u00e4pi kajahtaa?",
                "",
                "M\u00e4 kuulen, tai niin luulen",
                "taajuuden tuon, tuonpuoleisen kai sain",
                "kuuletteko kuiskeen (tauko) se varoittaa:",
                "",
                "Ke\u2019en t\u00e4st\u00e4 k\u00e4y",
                "kaiken toivon heitt\u00e4\u00e4 saa,",
                "matka johdattaa",
                "k\u00e4rsimysten kaupunkiin",
                "iankaikkiseen,",
                "kadotetun kansan joukkoon",
                "joka tuomittu on ma\u2019an uumeniin"
            ]
        }
    ]
}